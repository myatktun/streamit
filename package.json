{
    "name": "streamit",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start": "nx serve",
        "build": "nx build",
        "lint": "nx lint",
        "lint:all": "nx run-many --all --target=lint",
        "test": "nx test",
        "deploy": "nx deploy",
        "deploy:all": "nx run-many --all --target=deploy",
        "dev:up": "docker compose -f docker-compose.dev.yml up --build -d",
        "dev:down": "docker compose -f docker-compose.dev.yml down -v && docker image prune -f",
        "dev:restart": "docker compose -f docker-compose.dev.yml restart",
        "tf:dev": "terraform -chdir=./infra/dev",
        "tf:prod": "terraform -chdir=./infra/prod"
    },
    "private": true,
    "dependencies": {
        "@aws-sdk/client-s3": "^3.238.0",
        "amqplib": "^0.10.3",
        "express": "^4.18.1",
        "mongodb": "^4.11.0",
        "tslib": "^2.3.0",
        "wait-port": "^1.0.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.2.0",
        "@commitlint/config-conventional": "^17.2.0",
        "@nrwl/cli": "15.4.1",
        "@nrwl/eslint-plugin-nx": "15.4.1",
        "@nrwl/express": "15.4.1",
        "@nrwl/jest": "15.4.1",
        "@nrwl/linter": "15.4.1",
        "@nrwl/node": "15.4.1",
        "@nrwl/workspace": "15.4.1",
        "@types/amqplib": "^0.10.0",
        "@types/express": "4.17.15",
        "@types/jest": "29.2.4",
        "@types/node": "18.11.17",
        "@types/serve-favicon": "^2.5.3",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "body-parser": "^1.20.1",
        "eslint": "~8.30.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.2",
        "jest": "29.3.1",
        "jest-environment-jsdom": "29.3.1",
        "lint-staged": "^13.0.3",
        "nx": "15.4.1",
        "prettier": "^2.6.2",
        "supertest": "^6.3.1",
        "ts-jest": "29.0.3",
        "ts-node": "10.9.1",
        "typescript": "~4.9.4"
    }
}
