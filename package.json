{
    "name": "streamit",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start": "nx serve",
        "build": "nx build",
        "lint": "nx lint",
        "lint:all": "nx run-many --all --target=lint",
        "test": "nx test",
        "deploy": "nx deploy",
        "deploy:all": "nx run-many --all --target=deploy",
        "dev:up": "docker compose -f docker-compose.dev.yml up --build -d",
        "dev:down": "docker compose -f docker-compose.dev.yml down -v && docker image prune -f"
    },
    "private": true,
    "dependencies": {
        "@aws-sdk/client-s3": "^3.208.0",
        "express": "^4.18.1",
        "mongodb": "^4.11.0",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.2.0",
        "@commitlint/config-conventional": "^17.2.0",
        "@nrwl/cli": "15.0.13",
        "@nrwl/eslint-plugin-nx": "15.0.13",
        "@nrwl/express": "15.0.13",
        "@nrwl/jest": "15.0.13",
        "@nrwl/linter": "15.0.13",
        "@nrwl/node": "15.0.13",
        "@nrwl/workspace": "15.0.13",
        "@types/express": "4.17.13",
        "@types/jest": "28.1.1",
        "@types/node": "18.7.1",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "eslint": "~8.15.0",
