        "husky": "^8.0.2",
        "jest": "28.1.1",
        "jest-environment-jsdom": "28.1.1",
        "lint-staged": "^13.0.3",
        "nx": "15.0.13",
        "prettier": "^2.6.2",
        "supertest": "^6.3.1",
        "ts-jest": "28.0.5",
        "ts-node": "10.9.1",
        "typescript": "~4.8.2"
    }
